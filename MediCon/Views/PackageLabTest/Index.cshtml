{
    <script src="~/Scripts/AngularJS/PackageLab/PackageLab.js?v=1"></script>
    <script src="~/Content/Daypilot-Calendar/scripts/daypilot-all.min.js"></script>
    <script src="~/Content/Flatpickr-DatePicker/flatpickr.js"></script>
}