                                        <select name="sig2" id="sig2" style="width: 100%; text-transform : uppercase" class="form-control" required="" tabindex="-1" aria-hidden="true" ng-model="uri.medtech" disabled>
                                            <option value="" disabled>Select Personel</option>
                                            <option ng-repeat="s in specialistData" value="{{s.personnelID}}">{{s.fullname}}</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <!-- /MODAL URINALYSIS RESULT -->

    @*MODAL ECG*@
    <div class="modal fade" id="modalECG">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header" style="background-color:#1ab394;color:white">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title text-center">ECG</h4>
                </div>
                <form role="form" name="ECGForm" id="ECGForm">
                    <div class="modal-body" style="height: 100%;">
                        <div class="form-group">
                            <label class="form-control" style="border:none" for="ecgFindings">Findings:</label>
                            <textarea class="form-control form-control-sm" rows="8" id="ecgFindings" name="ecgFindings" ng-model="ecg.findings" disabled required></textarea>
