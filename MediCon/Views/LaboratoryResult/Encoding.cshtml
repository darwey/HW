@{
    ViewBag.Title = "Encoding";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .swal2-container {
        z-index: 20000 !important;
    }

    /*.borderless tr td {
        border: none !important;
        padding: 2px !important;
    }*/

     .table-wrapper-scroll-y {
        display: block;
    }

     th {
	    position: sticky;
	    position: -webkit-sticky;
	    top: 0px;
	    z-index: 2;
    }

    .my-custom-scrollbar {
        position: relative;
        /*height: 760px !important;*/
        overflow: auto;
        border: none;
        width: 100%;
    }

    input#pusCells {
        width: 55px;
    }

    .swal2-popup {
        font-size: 1.3em !important;
    }
</style>

<div ng-controller="LabResultCtrl">
    <!-- HEADER -->
    <div class="row wrapper border-bottom white-bg page-heading">
        <div class="col-lg-10">
            <h2>Laboratory</h2>
            <ol class="breadcrumb">
                <li>
                    <a href="">Medicon</a>
                </li>
                <li class="active">
                    <strong>Laboratory Result</strong>
                </li>
            </ol>
        </div>
        <div class="col-sm-2" style="margin-top:30px; text-align:end;">
            <button class="btn" ng-class="showClientList == false ? 'btn-primary':'btn-danger'" ng-show="showClientListBTN" ng-click="showDiagnoseList()"><i class="fa" ng-class="showClientList == false ? 'fa-list':'fa-angle-double-left'"></i> {{showClientList == false ? 'Patient List':'Back'}} </button>
        </div>
    </div>
    <!-- /HEADER -->


    <div class="wrapper wrapper-content ">
        <!-- QR CODE INFO FORM -->
        <div class="animated fadeInRight ecommerce" ng-show="!showClientList">
            <div class="row">
                <div class="col-lg-5 col-md-5">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title text-center">
                            <label style="margin-right: 20px;">SEARCH BY: </label>
                            <div class="radio radio-primary radio-inline">
                                <input type="radio" id="radio_byQR" name="radio_byQR" value="byQR" ng-model="searchBy" ng-change="selectSearchBy(searchBy)" />
                                <label for="radio_byQR"> <b>QR CODE</b> </label>
                            </div>
                            <div class="radio radio-danger radio-inline">
                                <input type="radio" id="radio_byName" name="radio_byName" value="byName" ng-model="searchBy" ng-change="selectSearchBy(searchBy)" />
                                <label for="radio_byName"> <b>NAME</b> </label>
                            </div>
                        </div>
                        <div class="ibox-content">
                            <div ng-show="searchBy == 'byQR'">
                                <div class="profile_img">
                                    <!-- Current avatar -->
                                    <video id="preview" class="img-responsive avatar-view" style="width: 100%; height: 100%;" />
                                </div>
                                <form name="qrSearchForm">
                                    <div class="input-group" style="margin-top: 10px; margin-bottom: -10px;">
                                        <input type="text" class="form-control border-green" ng-model="searchQRcode" placeholder="Search by QR code number" style="width:100%;" required>
                                        <span class="input-group-btn">
                                            <button type="button" class="btn btn-success" ng-disabled="qrSearchForm.$invalid" ng-click="mainSearch(searchQRcode)"> Find <i class="fa fa-qrcode"></i></button>
                                        </span>
                                    </div>
                                </form>
                            </div>
                            <br />

                            <div class="text-center" ng-show="loader" style="margin-top: 10%;">
                                <div class="sk-spinner sk-spinner-double-bounce">
                                    <div class="sk-double-bounce1"></div>
                                    <div class="sk-double-bounce2"></div>
                                </div>
                                <text><i>Please wait, we are getting your data...</i></text>
                            </div>
                            <div ng-show="!loader">
                                <button class="btn btn-primary full-width" ng-show="showBackBtn" ng-click="showInfoForm = !showInfoForm; showBackBtn = !showBackBtn"><i class="fa fa-search"></i> Back to Search </button>
                                <form id="QR_FORM" name="QR_FORM" ng-show="showInfoForm">
                                    <div class="row">
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label" for="qr_lastName">Last name</label>
                                                <input type="text" id="qr_lastName" name="qr_lastName" ng-model="qrData.lastName" placeholder="Last name" class="form-control text-center" readonly style="background-color: white;" />
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label" for="qr_firstName">First name</label>
                                                <input type="text" id="qr_firstName" name="qr_firstName" ng-model="qrData.firstName" placeholder="First name" class="form-control text-center" readonly style="background-color: white;" />
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label" for="qr_mi">Middle name</label>
                                                <input type="text" id="qr_mi" name="qr_mi" ng-model="qrData.middleName" placeholder="Middle name" class="form-control text-center" readonly style="background-color: white;" />
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label" for="qr_suffix">Suffix</label>
                                                <input type="text" id="qr_suffix" name="qr_suffix" ng-model="qrData.extName" placeholder="Suffix" class="form-control text-center" readonly style="background-color: white;" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label" for="qr_birthdate">Birthday</label>
                                                <input type="date" id="qr_birthdate" name="qr_birthdate" ng-model="qrData.birthdate" class="form-control text-center" readonly style="background-color: white;" />
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <label class="control-label">Sex</label>
                                            <div class="form-group text-center">
                                                <div class="radio radio-primary radio-inline">
                                                    <input type="radio" id="radio_sex_male" value="true" name="radio_sex" ng-model="qrData.sex" disabled />
                                                    <label for="radio_sex_male"> Male </label>
                                                </div>
                                                <div class="radio radio-danger radio-inline">
                                                    <input type="radio" id="radio_sex_female" value="false" name="radio_sex" ng-model="qrData.sex" disabled />
                                                    <label for="radio_sex_female"> Female </label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-sm-12">
                                            <div class="form-group">
                                                <label class="control-label" for="qr_contactNo">Contact no.</label>
                                                <input type="text" id="qr_contactNo" name="qr_contactNo" ng-model="qrData.contactNo" placeholder="Contact no." class="form-control text-center" readonly style="background-color: white;" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <div class="form-group">
                                                <label class="control-label" for="qr_address">Address</label>
                                                <input type="text" id="qr_address" name="qr_address" ng-model="qrData.fullAddress" placeholder="Address" class="form-control text-center" readonly style="background-color: white;" />
                                            </div>
                                        </div>
                                    </div>
                                </form>

                                <div ng-show="!showInfoForm">
                                    <form id="INFO_FORM" name="INFO_FORM">
                                        <div class="row">
                                            <div class="col-sm-6">
                                                <div class="form-group">
                                                    <input type="text" id="info_lastName" name="info_lastName" ng-model="infoFormData.lastName" placeholder="Last name" class="form-control text-center" />
                                                </div>
                                            </div>
                                            <div class="col-sm-6">
                                                <div class="form-group">
                                                    <input type="text" id="info_firstName" name="info_firstName" ng-model="infoFormData.firstName" placeholder="First name" class="form-control text-center" />
                                                </div>
                                            </div>
                                            <div class="col-sm-12">
                                                <button class="btn btn-primary full-width" ng-disabled="INFO_FORM.$invalid" ng-click="mainSearchByName(infoFormData)"><i class="fa fa-search"></i> Search Record </button>
                                            </div>
                                        </div>
                                    </form>
                                    <hr />
                                    <div class="table-wrapper-scroll-y my-custom-scrollbar" style="height: 300px !important;">
                                        <table class="table table-stripped table-bordered">
                                            <thead>
                                                <tr>
                                                    <th>Last name</th>
                                                    <th>First name</th>
                                                    <th>Middle name</th>
                                                    <th>Sex</th>
                                                    <th>Birthday</th>
                                                    <th class="text-center">Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr class="text-center" ng-show="tableLoader">
                                                    <td colspan="6">
                                                        <div class="sk-spinner sk-spinner-double-bounce">
                                                            <div class="sk-double-bounce1"></div>
                                                            <div class="sk-double-bounce2"></div>
                                                        </div>
                                                        <text><i>Please wait, we are loading your data...</i></text>
                                                    </td>
                                                </tr>
                                                <tr class="text-center" ng-show="searchResultList.length == 0 && tableLoader == false">
                                                    <td colspan="6">
                                                        <label class="text-danger">NO INFORMATION FOUND!</label>
                                                    </td>
                                                </tr>
                                                <tr ng-repeat="person in searchResultList | orderBy: 'firstName'">
                                                    <td>{{person.lastName}}</td>
                                                    <td>{{person.firstName}}</td>
                                                    <td>{{person.middleName}}</td>
                                                    <td>{{person.sex == 'MALE' ? 'M' : person.sex == 'FEMALE' ? 'F' : ''}}</td>
                                                    <td>{{person.birthDate}}</td>
                                                    <td class="text-center">
                                                        <button class="btn btn-sm btn-primary" ng-click="selectPerson(person)">Select</button>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
                <div class="col-lg-7 col-md-7">
                    <div class="panel panel-success" style="min-height: 210px; height: 840px;">
                        <div class="panel-heading text-center">
                            <label class="text-center">LABORATORY HISTORY</label>
                        </div>
                        <div class="panel-body">
                            <div class="table-wrapper-scroll-y my-custom-scrollbar" style="height: 760px !important;">
                                <table class="table table-stripped table-bordered">
                                    <thead>
                                        <tr>
                                            <th class="">#</th>
                                            <th>Test</th>
                                            <th>Lab. Personnel</th>
                                            <th>Lab. Date & Time</th>
                                            <th>Referred by</th>
                                            <th>Date & Time</th>
                                            <th>Status</th>
                                            <th>Action</th>

                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr class="text-center" ng-show="loader">
                                            <td colspan="8">
                                                <div class="sk-spinner sk-spinner-double-bounce">
                                                    <div class="sk-double-bounce1"></div>
                                                    <div class="sk-double-bounce2"></div>
                                                </div>
                                                <text><i>Please wait, we are loading your data...</i></text>
                                            </td>
                                        </tr>
                                        <tr class="text-center" ng-show="bpHistoryList.length == 0 && bpLoader == false">
                                            <td colspan="6">
                                                <label class="text-danger">NO INFORMATION FOUND!</label>
                                            </td>
                                        </tr>
                                        <tr ng-repeat="hist in testHistory | orderBy: '-labDT'">
                                            <td>{{$index + 1}}</td>
                                            <td>{{hist.labTestName}}</td>
                                            <td>{{hist.labPersonnel[0].personnel_firstName}} {{hist.labPersonnel[0].personnel_middleName}} {{hist.labPersonnel[0].personnel_lastName}} {{hist.labPersonnel[0].personnel_extName}}</td>
                                            <td>{{hist.labDT}}</td>
                                            <td>{{hist.consultPersonnel[0].personnel_firstName}} {{hist.consultPersonnel[0].personnel_middleName}} {{hist.consultPersonnel[0].personnel_lastName}} {{hist.consultPersonnel[0].personnel_extName}}</td>
                                            <td>{{hist.consultDT}}</td>
                                            <td><b ng-class="hist.isEncoded == true ? 'text-success' : 'text-danger'">{{hist.isEncoded == true ? 'Encoded' : 'Not Encoded'}}</b></td>
                                            <td>
                                                <button ng-disabled="hist.isEncoded == true" class="btn btn-success btn-sm" ng-click="encodeResult(hist, 0, true)">Encode</button>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
        <!-- /QR CODE INFO FORM -->

        <!-- LIST OF PATIENTS -->
        <div class="animated fadeInRight ecommerce" ng-show="showClientList">
            <div class="col-lg-12">
                <form class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-2 control-label" style="text-align:center !important">Laboratory Date:</label>
                        <div class="col-sm-2">
                            <input type="date" id="FilterDate" ng-change="filterResult(FilterDate)" name="FilterDate" style="text-transform:uppercase" ng-model="FilterDate" class="text-center form-control">
                        </div>
                    </div>
                </form>
            </div>
            <br />
            <div class="ibox float-e-margins">
                <div class="ibox-title text-center">
                    <label class="text-center">LIST OF PATIENTS</label>
                </div>
                <div class="ibox-content">
                    <div class="table-wrapper-scroll-y my-custom-scrollbar">
                        <table id="clientList_tbl" class="table table-stripped table-hover" style="width: 100%;">
                            <thead>
                                <tr>

                                    <th>#</th>
                                    <th>Last name</th>
                                    <th>First name</th>
                                    <th>M.I.</th>
                                    <th>Suffix</th>
                                    <th>Sex</th>
                                    <th>Age</th>
                                    <th>Contact no.</th>
                                    <th>Lab. Test</th>
                                    <th>Lab. Date & Time</th>
                                    <th>Referred by</th>
                                    <th>Date Encoded</th>
                                    <th>Encoded By</th>
                                    <th class="text-right" data-sort-ignore="true">Action</th>

                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <!-- /LIST OF PATIENTS -->

        <!-- Modal BLoodchem -->
        <div class="modal fade" id="modalBLoodChem">
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <div class="modal-header" style="background-color:#1ab394;color:white">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title text-center">BLOOD CHEMISTRY</h4>
                    </div>
                    <form role="form" id="bloodChemForm" name="bloodChemForm">
                        <div class="modal-body" style="height: 100%;">
                            @*<div class="table-wrapper-scroll-y my-custom-scrollbar">*@
                            <table class="table table-bordered">
                                <thead>
                                    <tr>
                                        <th width="30%">TEST NAME</th>
                                        <th width="45%">RESULT</th>
                                        <th width="25%">REFERENCE VALUE</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr ng-show="tempLabTestID == 'L0005'">
                                        <td>FBS</td>
                                        <td>
                                            <div class="form-group " style="margin:0px">
                                                <div class="input-group">
                                                    <input type="number" step="0.01" class="form-control form-control-sm" id="fbs" name="fbs" placeholder="Enter result" ng-model="blood.fbs" ng-required="tempLabTestID == 'L0005'" ng-disabled="!isShowResult">
                                                    <span class="input-group-addon" style="background-color:#F5F5F6">mmol/L</span>
                                                </div>
                                            </div>
                                        </td>
                                        <td>
                                            3.9 - 5.8 mmol/L
                                        </td>
                                    </tr>
                                    <tr ng-show="tempLabTestID == 'L0010'">
                                        <td>SERUM URIC ACID</td>
                                        <td>
                                            <div class="form-group" style="margin:0px">
                                                <div class="input-group">
                                                    <input type="number" class="form-control form-control-sm" id="serumUricAcid" name="serumUricAcid" placeholder="Enter result" ng-model="blood.serumUricAcid" ng-required="tempLabTestID == 'L0010'" ng-disabled="!isShowResult">
                                                    <span class=" input-group-addon" style="background-color:#F5F5F6">umol/L</span>
                                                </div>
                                            </div>
                                        </td>
                                        <td>
                                            <b>MALE:</b> 208 - 430 umol/L
                                            <hr style="margin:0px" />
                                            <b>FEMALE:</b> 155 - 360 umol/L
                                        </td>
                                    </tr>
                                    <tr ng-show="tempLabTestID == 'L0009'">
                                        <td>CREATININE</td>
                                        <td>
                                            <div class="form-group" style="margin:0px">
                                                <div class="input-group">
                                                    <input type="number" class="form-control form-control-sm" id="creatinine" name="creatinine" placeholder="Enter result" ng-model="blood.creatinine" ng-required="tempLabTestID == 'L0009'" ng-disabled="!isShowResult">
                                                    <span class="input-group-addon" style="background-color:#F5F5F6">mmol/L</span>
                                                </div>
                                            </div>
                                        </td>
                                        <td>
                                            <b>MALE:</b> 62 - 115 umol/L
                                            <hr style="margin:0px" />
                                            <b>FEMALE:</b> 53 - 97 umol/L
                                        </td>
                                    </tr>
                                    <tr ng-show="tempLabTestID == 'L0007'">
                                        <td>CHOLESTEROL</td>
                                        <td>
                                            <div class="form-group" style="margin:0px">
                                                <div class="input-group">
                                                    <input type="number" step="0.01" min="3.6" max="5.7" class="form-control form-control-sm" id="cholesterol" name="cholesterol" placeholder="Enter result" ng-model="blood.cholesterol" ng-required="tempLabTestID == 'L0007'" ng-disabled="!isShowResult">
                                                    <span class="input-group-addon" style="background-color:#F5F5F6">mmol/L</span>
                                                </div>
                                            </div>
                                        </td>
                                        <td>
                                            3.6 - 5.7 mmol/L
                                        </td>
                                    </tr>
                                    <tr ng-show="tempLabTestID == 'L0007'">
                                        <td>TRIGLYCERIDES</td>
                                        <td>
                                            <div class="form-group" style="margin:0px">
                                                <div class="input-group">
                                                    <input type="number" step="0.01" min="0.7" max="2.26" class="form-control form-control-sm" id="triglycerides" name="triglycerides" placeholder="Enter result" ng-model="blood.triglycerides" ng-required="tempLabTestID == 'L0007'" ng-disabled="!isShowResult">
                                                    <span class="input-group-addon" style="background-color:#F5F5F6">mmol/L</span>
                                                </div>
                                            </div>
                                        </td>
                                        <td>
                                            0.7 - 2.26 mmol/L
                                        </td>
                                    </tr>
                                    <tr ng-show="tempLabTestID == 'L0007'">
                                        <td>HDL</td>
                                        <td>
                                            <div class="form-group" style="margin:0px">
                                                <div class="input-group">
                                                    <input type="number" step="0.01" min="0.79" max="2" class="form-control form-control-sm" id="hdl" name="hdl" placeholder="Enter result" ng-model="blood.hdl" ng-required="tempLabTestID == 'L0007'" ng-disabled="!isShowResult">
                                                    <span class="input-group-addon" style="background-color:#F5F5F6">mmol/L</span>
                                                </div>
                                            </div>
                                        </td>
                                        <td>
                                            0.79 - 1.94 mmol/L
                                        </td>
                                    </tr>
                                    <tr ng-show="tempLabTestID == 'L0007'">
                                        <td>LDL</td>
                                        <td>
                                            <div class="form-group" style="margin:0px">
                                                <div class="input-group">
                                                    <input type="number" step="0.01" min="0.78" max="3" class="form-control form-control-sm" id="ldl" name="ldl" placeholder="Enter result" ng-model="blood.ldl" ng-required="tempLabTestID == 'L0007'" ng-disabled="!isShowResult">
                                                    <span class="input-group-addon" style="background-color:#F5F5F6">mmol/L</span>
                                                </div>
                                            </div>
                                        </td>
                                        <td>
                                            0.78 - 2.9 mmol/L
                                        </td>
                                    </tr>
                                    <tr ng-show="tempLabTestID == 'L0007'">
                                        <td>VLDL</td>
                                        <td>
                                            <div class="form-group" style="margin:0px">
                                                <div class="input-group">
                                                    <input type="number" step="0.01" min="0.10" max="2" class="form-control form-control-sm" id="vldl" name="vldl" placeholder="Enter result" ng-model="blood.vldl" ng-required="tempLabTestID == 'L0007'" ng-disabled="!isShowResult">
                                                    <span class="input-group-addon" style="background-color:#F5F5F6">mmol/L</span>
                                                </div>
                                            </div>
                                        </td>
                                        <td>
                                            0.10 - 1.70 mmol/L
                                        </td>
                                    </tr>
                                    <tr ng-show="tempLabTestID == 'L0011'">
                                        <td>SGPT</td>
                                        <td>
                                            <div class="form-group" style="margin:0px">
                                                <div class="input-group">
                                                    <input type="number" min="4" max="41" class="form-control form-control-sm" id="sgpt" name="sgpt" placeholder="Enter result" ng-model="blood.sgpt" ng-required="tempLabTestID == 'L0011'" ng-disabled="!isShowResult">
                                                    <span class="input-group-addon" style="background-color:#F5F5F6">U/L</span>
                                                </div>
                                            </div>
                                        </td>
                                        <td>
                                            <b>MALE:</b> 4 - 41 U/L
                                            <hr style="margin:0px" />
                                            <b>FEMALE:</b> 4 - 31 U/L
                                        </td>
                                    </tr>
                                    <tr ng-show="tempLabTestID == 'L0012'">
                                        <td>SGOT</td>
                                        <td>
                                            <div class="form-group" style="margin:0px">
                                                <div class="input-group">
                                                    <input type="number" min="6" max="37" class="form-control form-control-sm" id="sgot" name="sgot" placeholder="Enter result" ng-model="blood.sgot" ng-required="tempLabTestID == 'L0012'" ng-disabled="!isShowResult">
                                                    <span class="input-group-addon" style="background-color:#F5F5F6">mmol/L</span>
                                                </div>
                                            </div>
                                        </td>
                                        <td>
                                            <b>MALE:</b> 6 - 37 U/L
                                            <hr style="margin:0px" />
                                            <b>FEMALE:</b> 6 - 31 U/L
                                        </td>
                                    </tr>
                                    @*<tr>
                                        <td>BUN</td>
                                        <td>
                                            <div class="form-group" style="margin:0px">
                                                <div class="input-group">
                                                    <input type="number" class="form-control form-control-sm" id="bun" name="bun" placeholder="Enter result" ng-model="blood.bun">
                                                    <span class="input-group-addon" style="background-color:#F5F5F6">mmol/L</span>
                                                </div>

                                            </div>
                                        </td>
                                        <td>
                                            2.2 - 7.1 mmol/L
                                        </td>
                                    </tr>*@
                                    <tr ng-show="tempLabTestID == 'L0008'">
                                        <td>POTASSIUM (K<sup>++</sup>)</td>
                                        <td>
                                            <div class="form-group" style="margin:0px">
                                                <div class="input-group">
                                                    <input type="number" step="0.01" min="3.50" max="5.30" class="form-control form-control-sm" id="potassium" name="potassium" placeholder="Enter result" ng-model="blood.potassium" ng-required="tempLabTestID == 'L0008'" ng-disabled="!isShowResult">
                                                    <span class="input-group-addon" style="background-color:#F5F5F6">mmol/L</span>
                                                </div>

                                            </div>
                                        </td>
