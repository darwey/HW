@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    .table-wrapper-scroll-y {
        display: block;
    }

    .my-custom-scrollbar {
        position: relative;
        height: 400px !important;
        overflow: auto;
        border: none;
        width: 100%;
    }

    th {
	    position: sticky;
	    position: -webkit-sticky;
	    top: 0px;
	    z-index: 2;
    }

      .select2-container .select2-selection {
        height: 34px;
    }

    .select2-container--default .select2-selection--single .select2-selection__rendered {
        color: #444;
        line-height: 32px;
    }

    .tabs-container .nav-tabs > li.active > a, .tabs-container .nav-tabs > li.active > a:hover, .tabs-container .nav-tabs > li.active > a:focus {
        background-color: #1ab394;
        color: white;
    }

    u {
        color: royalblue;
    }
</style>


<div ng-controller="RectalDiagnosisCtrl">
    <!-- HEADER -->
    <div class="row wrapper border-bottom white-bg page-heading">
        <div class="col-lg-10">
            <h2>Rectal Diagnosis</h2>
            <ol class="breadcrumb">
                <li>
                    <a href="">Medicon</a>
                </li>
                <li class="active">
                    <strong>Rectal Diagnosis</strong>
                </li>
            </ol>
        </div>
        <div class="col-sm-2" style="margin-top:30px; text-align:end;">
            <button class="btn" ng-class="showClientList == false ? 'btn-primary':'btn-danger'" ng-show="showBtnClientList" ng-click="showDiagnoseList('viewList')"><i class="fa" ng-class="showClientList == false ? 'fa-list':'fa-angle-double-left'"></i> {{showClientList == false ? 'Patient List':'Back'}} </button>
            <button class="btn btn-danger" ng-show="!showBtnClientList" ng-click="showDiagnoseList('returnHome')"><i class="fa fa-angle-double-left"></i> Back </button>
        </div>
    </div>
    <!-- /HEADER -->

    <div class="wrapper wrapper-content ">
        <!-- MAIN INFORMATION PANEL -->
        <div class="animated fadeInRight ecommerce" ng-show="!showClientList && !diagnosePanel">
            <div class="row">
                <!-- QR CODE INFO FORM -->
                <div class="col-lg-5 col-md-5">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title text-center">
                            <label class="text-center">QR CODE INFORMATION</label>
                        </div>
                        <div class="ibox-content">
                            <div class="profile_img">
                                <!-- Current avatar -->
                                <video id="preview" class="img-responsive avatar-view" style="width: 100%; height: 100%;" />
                            </div>
                            <form name="qrSearchForm">
                                <div class="input-group" style="margin-top: 10px; margin-bottom: -10px;">
                                    <input type="text" class="form-control border-green" ng-model="searchQRcode" placeholder="Search by QR code number" style="width:100%;" required>
                                    <span class="input-group-btn">
                                        <button type="button" class="btn btn-success" ng-disabled="qrSearchForm.$invalid" ng-click="mainSearch(searchQRcode)"> Find <i class="fa fa-qrcode"></i></button>
                                    </span>
                                </div>
                            </form>
                            <br />

                            <div class="text-center" ng-show="loader" style="margin-top: 10%;">
                                <div class="sk-spinner sk-spinner-double-bounce">
                                    <div class="sk-double-bounce1"></div>
                                    <div class="sk-double-bounce2"></div>
                                </div>
                                <text><i>Please wait, we are getting your data...</i></text>
                            </div>
                            <div ng-show="!loader">
                                <form id="QR_FORM" name="QR_FORM">
                                    <div class="row">
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label" for="qr_lastName">Last name</label>
                                                <input type="text" id="qr_lastName" name="qr_lastName" ng-model="qrData.lastName" placeholder="Last name" class="form-control text-center" readonly style="background-color: white;" />
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label" for="qr_firstName">First name</label>
                                                <input type="text" id="qr_firstName" name="qr_firstName" ng-model="qrData.firstName" placeholder="First name" class="form-control text-center" readonly style="background-color: white;" />
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label" for="qr_mi">Middle name</label>
                                                <input type="text" id="qr_mi" name="qr_mi" ng-model="qrData.middleName" placeholder="Middle name" class="form-control text-center" readonly style="background-color: white;" />
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label" for="qr_suffix">Suffix</label>
                                                <input type="text" id="qr_suffix" name="qr_suffix" ng-model="qrData.extName" placeholder="Suffix" class="form-control text-center" readonly style="background-color: white;" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label" for="qr_birthdate">Birthday</label>
                                                <input type="date" id="qr_birthdate" name="qr_birthdate" ng-model="qrData.birthdate" class="form-control text-center" readonly style="background-color: white;" />
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <label class="control-label">Sex</label>
                                            <div class="form-group text-center">
                                                <div class="radio radio-primary radio-inline">
                                                    <input type="radio" id="radio_sex_male" value="true" name="radio_sex" ng-model="qrData.sex" disabled />
                                                    <label for="radio_sex_male"> Male </label>
                                                </div>
                                                <div class="radio radio-danger radio-inline">
                                                    <input type="radio" id="radio_sex_female" value="false" name="radio_sex" ng-model="qrData.sex" disabled />
                                                    <label for="radio_sex_female"> Female </label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-sm-12">
                                            <div class="form-group">
                                                <label class="control-label" for="qr_contactNo">Contact no.</label>
                                                <input type="text" id="qr_contactNo" name="qr_contactNo" ng-model="qrData.contactNo" placeholder="Contact no." class="form-control text-center" readonly style="background-color: white;" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <div class="form-group">
                                                <label class="control-label" for="qr_address">Address</label>
                                                <input type="text" id="qr_address" name="qr_address" ng-model="qrData.fullAddress" placeholder="Address" class="form-control text-center" readonly style="background-color: white;" />
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>

                    </div>
                </div>
                <!-- /QR CODE INFO FORM -->

                <!-- MAIN INFORMATION PANEL (DIGITAL RECTAL EXAM, VITAL SIGNS INFO, RECTAL DIAGNOSIS HISTORY, RX HISTORY, LAB HISTORY) -->
                <div class="col-lg-7 col-md-7">
                    <div class="row">
                        <div class="col-lg-6 col-md-6">
                            <div class="panel panel-warning">
                                <div class="panel-heading text-center">
                                    <label class="text-center">DIGITAL RECTAL EXAMINATION</label>
                                </div>
                                <div class="panel-body" style="height: 303px;">
                                    <div class="table-wrapper-scroll-y" style="position: relative; height: 270px !important; overflow: auto; border: none; width: 100%;">
                                        <table class="table table-stripped">
                                            <thead>
                                                <tr>

                                                    <th class="">#</th>
                                                    <th>Interviewed by</th>
                                                    <th>Date & Time</th>
                                                    <th class="text-center">Action</th>

                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr class="text-center" ng-show="rectalExamLoader">
                                                    <td colspan="4">
                                                        <div class="sk-spinner sk-spinner-double-bounce">
                                                            <div class="sk-double-bounce1"></div>
                                                            <div class="sk-double-bounce2"></div>
                                                        </div>
                                                        <text><i>Please wait, we are loading your data...</i></text>
                                                    </td>
                                                </tr>
                                                <tr class="text-center" ng-show="rectalExamList.length == 0 && rectalExamLoader == false">
                                                    <td colspan="6">
                                                        <label class="text-danger">NO INFORMATION FOUND!</label>
                                                    </td>
                                                </tr>
                                                <tr ng-repeat="exam in rectalExamList">
                                                    <td>{{$index + 1}}</td>
                                                    <td>{{exam.interviewPerson[0].personnel_firstName}} {{exam.interviewPerson[0].personnel_middleName}} {{exam.interviewPerson[0].personnel_lastName}} {{exam.interviewPerson[0].personnel_extName}}</td>
                                                    <td>{{exam.interviewDT}}</td>
                                                    <td class="text-center">
                                                        <button class="btn btn-warning" ng-disabled="exam.isDiagnoseDone == true" ng-click="proceedDiagnosis(false, exam)">Proceed</button>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-6">
                            <div class="panel panel-success" style="min-height: 200px; height: 350px;">
                                <div class="panel-heading text-center">
                                    <label class="text-center">VITAL SIGNS INFORMATION</label>
                                </div>
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-xs-6">
                                            <div class="input-group m-b"><span class="input-group-addon" style="font-weight: 700;">Height</span> <input type="text" class="form-control text-center" ng-model="vitalSigns.height" readonly style="font-weight: 700;"></div>
                                        </div>
                                        <div class="col-xs-6">
                                            <div class="input-group m-b"><span class="input-group-addon" style="font-weight: 700;">Weight</span> <input type="text" class="form-control text-center" ng-model="vitalSigns.weight" readonly style="font-weight: 700;"></div>
                                        </div>
                                        <div class="col-xs-12">
                                            <div class="input-group m-b">
                                                <span class="input-group-addon" style="font-weight: 700;">BMI</span>
                                                <input type="text" class="form-control text-center" ng-class="BMI.desc == 'UNDERWEIGHT' ? 'text-success' : BMI.desc == 'NORMAL' ? 'text-primary' : BMI.desc == 'OVERWEIGHT' ? 'text-warning' : 'text-danger'" ng-model="BMI.data" readonly style="font-weight: 700;">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="table-wrapper-scroll-y" style="position: relative; height: 170px !important; overflow: auto; border: none; width: 100%;">
                                        <table class="table table-stripped">
                                            <thead>
                                                <tr>

                                                    <th class="">#</th>
                                                    <th>Temp.</th>
                                                    <th>BP</th>
                                                    <th>Pulse Rate</th>
                                                    <th>Date & Time</th>

                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr class="text-center" ng-show="bpLoader">
                                                    <td colspan="6">
                                                        <div class="sk-spinner sk-spinner-double-bounce">
                                                            <div class="sk-double-bounce1"></div>
                                                            <div class="sk-double-bounce2"></div>
                                                        </div>
                                                        <text><i>Please wait, we are loading your data...</i></text>
                                                    </td>
                                                </tr>
                                                <tr class="text-center" ng-show="bpHistoryList.length == 0 && bpLoader == false">
                                                    <td colspan="6">
                                                        <label class="text-danger">NO INFORMATION FOUND!</label>
                                                    </td>
                                                </tr>
                                                <tr ng-repeat="vs in bpHistoryList">
                                                    <td>{{$index + 1}}</td>
                                                    <td>{{vs.temperature}}</td>
                                                    <td>{{vs.systolic}}/{{vs.diastolic}}</td>
                                                    <td>{{vs.pulseRate}}</td>
                                                    <td>{{vs.dateTimeLog }}</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div>
                        <div id="tabs" class="tabs-container">

                            <ul class="nav nav-tabs">
                                <li class="active"><a data-toggle="tab" href="#tab-MDHistory"><i class="fa fa-stethoscope"></i> Rectal Diagnosis History </a></li>
                                <li class=""><a data-toggle="tab" href="#tab-MPHistory"><i class="fa fa-medkit"></i> Prescription</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-LabHistory"><i class="fa fa-eyedropper"></i> Laboratory</a></li>
                            </ul>
                            <div class="tab-content ">
                                <div id="tab-MDHistory" class="tab-pane active">
                                    <div class="panel-body" style="height: 430px;">
                                        <div class="table-wrapper-scroll-y my-custom-scrollbar">
                                            <table class="table table-stripped table-hover table-bordered" style="width: 100%;">
                                                <thead>
                                                    <tr>
                                                        <th class="text-center" style="color: #1ab394;">#</th>
                                                        <th style="color: #1ab394;">Diagnosed Date & Time</th>
                                                        <th style="color: #1ab394;">Physician</th>
                                                        <th style="color: #1ab394;">Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr class="text-center" ng-show="medicalRecord_loader">
                                                        <td colspan="7">
                                                            <div class="sk-spinner sk-spinner-double-bounce">
                                                                <div class="sk-double-bounce1"></div>
                                                                <div class="sk-double-bounce2"></div>
                                                            </div>
                                                            <text><i>Please wait, we are loading your data...</i></text>
                                                        </td>
                                                    </tr>
                                                    <tr class="text-center" ng-show="rectalHistoryList.length == 0 && medicalRecord_loader == false">
                                                        <td colspan="7">
                                                            <label class="text-danger">NO INFORMATION FOUND!</label>
                                                        </td>
                                                    </tr>
                                                    <tr ng-repeat="dh in rectalHistoryList">
                                                        <td class="text-center">{{$index + 1}}</td>
                                                        <td>{{dh.diagnoseDT}}</td>
                                                        <td>
                                                            {{dh.diagPerson[0].personnel_firstName}} {{dh.diagPerson[0].personnel_midInit == null ? '' : dh.diagPerson[0].personnel_midInit}}
                                                            {{dh.diagPerson[0].personnel_lastName}} {{dh.diagPerson[0].personnel_extName == null ? '' : dh.diagPerson[0].personnel_extName}}
                                                        </td>
                                                        <td class="text-center">
                                                            <button class="btn btn-primary" ng-click="viewRecord(dh)">View</button>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                                <div id="tab-MPHistory" class="tab-pane">
                                    <div class="panel-body" style="height: 430px;">
                                        <div class="table-wrapper-scroll-y my-custom-scrollbar">
                                            <table class="table table-stripped table-hover table-bordered" style="width: 100%;">
                                                <thead>
                                                    <tr>
                                                        <th class="text-center" style="color: #1ab394;">#</th>
                                                        <th style="color: #1ab394;">Physician</th>
                                                        <th style="color: #1ab394;">Rx Date & Time</th>
                                                        <th style="color: #1ab394;">Service</th>
                                                        <th style="color: #1ab394;">Prescription/s</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr class="text-center" ng-show="medicalRecord_loader">
                                                        <td colspan="6">
                                                            <div class="sk-spinner sk-spinner-double-bounce">
                                                                <div class="sk-double-bounce1"></div>
                                                                <div class="sk-double-bounce2"></div>
                                                            </div>
                                                            <text><i>Please wait, we are loading your data...</i></text>
                                                        </td>
                                                    </tr>
                                                    <tr class="text-center" ng-show="rectalRxList.length == 0 && medicalRecord_loader == false">
                                                        <td colspan="6">
                                                            <label class="text-danger">NO INFORMATION FOUND!</label>
                                                        </td>
                                                    </tr>
                                                    <tr ng-repeat="rx in rectalRxList">
                                                        <td class="text-center">{{$index + 1}}</td>
                                                        <td>{{rx[0].physician[0].personnel_firstName}} {{rx[0].physician[0].personnel_midInit}} {{rx[0].physician[0].personnel_lastName}} {{rx[0].physician[0].personnel_extName}}</td>
                                                        <td>{{rx[0].dateTimeRx}}</td>
                                                        <td>{{rx[0].serviceName}}</td>
                                                        <td>
                                                            <table class="table table-stripped" style="width: 100%;">
                                                                <thead>
                                                                    <tr>
                                                                        <td>Medicine Name</td>
                                                                        <td>Dosage</td>
                                                                        <td># Days</td>
                                                                        <td>Qty</td>
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    <tr ng-repeat="rx in rx">
                                                                        <td>{{rx.productDesc}} {{rx.measurementDesc}}</td>
                                                                        <td>{{rx.dosage}} {{rx.unitDesc}} x {{rx.perDay}} a Day</td>
                                                                        <td>{{rx.noDay == 'maintenance' ? 'as Maintenance':rx.noDay == 'needed' ? 'as Needed':rx.noDay == 'single' ? 'Single Dose':rx.noDay + ' Day/s'}}</td>
                                                                        <td>{{rx.qtyRx}} {{rx.unitDesc == 'Suspension' || rx.unitDesc == 'Syrup' || rx.unitDesc == 'Drops' || rx.unitDesc == 'Bottle' ? 'Bottle/s':rx.unitDesc == 'Cream' ? 'Tube':''}}</td>
                                                                    </tr>
                                                                </tbody>
                                                            </table>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                                <div id="tab-LabHistory" class="tab-pane">
                                    <div class="panel-body" style="height: 430px;">
                                        <div class="table-wrapper-scroll-y my-custom-scrollbar">
                                            <table class="table table-stripped table-hover table-bordered" style="width: 100%;">
                                                <thead>
                                                    <tr>
                                                        <th class="text-center" style="color: #1ab394;">#</th>
                                                        <th style="color: #1ab394;">Test</th>
                                                        <th style="color: #1ab394;" class="text-center">Service</th>
                                                        <th style="color: #1ab394;" class="text-center">Tested</th>
                                                        <th style="color: #1ab394;" class="text-center">Date & Time Tested</th>
                                                        <th style="color: #1ab394;" class="text-center">With Result</th>
                                                        <th style="color: #1ab394;" class="text-center">Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr class="text-center" ng-show="labLoader">
                                                        <td colspan="6">
                                                            <div class="sk-spinner sk-spinner-double-bounce">
                                                                <div class="sk-double-bounce1"></div>
                                                                <div class="sk-double-bounce2"></div>
                                                            </div>
                                                            <text><i>Please wait, we are loading your data...</i></text>
                                                        </td>
                                                    </tr>
                                                    <tr class="text-center" ng-show="labHistoryList.length == 0 && labLoader == false">
                                                        <td colspan="6">
                                                            <label class="text-danger">NO INFORMATION FOUND!</label>
                                                        </td>
                                                    </tr>
                                                    <tr ng-repeat="dh in labHistoryList | orderBy: '-dateTested'">
                                                        <td class="text-center">{{$index + 1}}</td>
                                                        <td>
                                                            {{dh.labTestID == 'L0004' ? dh.labTestName + ' (' + dh.ecgDesc + ')' :
                                                            dh.labTestID == 'L0006' ? dh.labTestName + ' (' + dh.xrayDesc + ')' :
                                                            dh.labTestID == 'L0023' ? dh.labTestName + ' (' + dh.ultrasoundDesc + ')' : dh.labTestName}}
                                                        </td>
                                                        <td class="text-center">{{dh.MRDiagnosisID == null ? dh.ConsultServiceName : dh.refServiceName}}</td>
                                                        <td class="text-center">{{dh.isTested == true ? 'Yes':'No'}}</td>
                                                        <td class="text-center">{{dh.dateTested == 'Invalid date' ? '':dh.dateTested}}</td>
                                                        <td class="text-center">{{dh.isEncoded == true ? 'Yes':'No'}}</td>
                                                        <td class="text-center"><button class="btn btn-primary btn-sm" ng-disabled="!dh.isEncoded" ng-click="viewLabResult(dh)"> Show </button></td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
                <!-- MAIN INFORMATION PANEL (DIGITAL RECTAL EXAM, VITAL SIGNS INFO, RECTAL DIAGNOSIS HISTORY, RX HISTORY, LAB HISTORY) -->
            </div>
        </div>
        <!-- /MAIN INFORMATION PANEL -->

        <!-- RECTAL DIAGNOSIS PANEL -->
        <div class="animated fadeInRight ecommerce" ng-show="diagnosePanel && !showClientList">
            <div class="ibox float-e-margins">
                <div class="ibox-title text-center">
                    <label class="text-center">DIGITAL RECTAL EXAMINATION</label>
                </div>
                <div class="ibox-content" style="padding-bottom: 5px !important;">
                    <div class="row">
                        <div class="col-lg-9 col-md-9">
                            <div class="input-group m-b">
                                <span class="input-group-addon">Name</span>
                                <input type="text" class="form-control text-center" ng-model="mrh.fullName" readonly style="font-weight: 700;">
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3">
                            <div class="input-group m-b">
                                <span class="input-group-addon">Age</span>
                                <input type="text" class="form-control text-center" ng-model="mrh.age" readonly style="font-weight: 700;">
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="ibox float-e-margins">
                <div class="tabs-container">
                    <ul class="nav nav-tabs" id="tab-menu">
                        <li class="active"><a data-toggle="tab" href="#tab-diag" id="tabDiag"> Rectal Diagnosis </a></li>
                        <li class=""><a data-toggle="tab" href="#tab-lab" id="tabLab"> Laboratory </a></li>
                        <li class=""><a data-toggle="tab" href="#tab-med" id="tabMed"> Medication </a></li>
                    </ul>
                    <div class="tab-content">
                        <div id="tab-diag" class="tab-pane active">
                            <div class="panel-body">
                                <div class="row">
                                    <!-- INTERVIEW QUESTIONS -->
                                    <div class="col-lg-6 col-md-6">
                                        <div class="ibox-content" style="background-color: #f4f4f4 !important; border-radius: 5px;">
                                            <div class="form-group">
                                                <label>Ito ba ang una ninyong DRE? <u>{{mrh.is1stDRE ? 'Ou' : 'Hindi'}}</u></label>
                                            </div>
                                            <div class="form-group">
                                                <label>Gaano kadalas kayong magpaDRE? <u>{{mrh.DREfrequency == 1 ? '1 sa isang taon' : mrh.DREfrequency == 2 ? '2 sa isang taon' : 'Mahigit sa 2 sa isang taon'}}</u></label>
                                            </div>
                                            <div class="form-group">
                                                <label>Mayroon bang miyembro ng pamilya na nagkaroon na ng Kanser sa prostata? <u>{{mrh.isProstateCancer ? 'Ou' : 'Wala'}}</u></label>
                                            </div>
                                            <div class="form-group">
                                                <label>Mayroon bang miyembro ng pamilya na nagkaroon na ng anumang klase ng Kanser? <u>{{mrh.isAnyCancer ? 'Ou' : 'Wala'}}</u></label>
                                            </div>
                                            <div class="form-group" ng-show="mrh.isAnyCancer">
                                                <label>Anong Kanser? <u>{{mrh.cancerName}}</u></label>
                                            </div>
                                            <div class="form-group">
                                                <label>Mayroon ba kayong iniinom na gamot sa kasalukuyan? <u>{{mrh.isMedication ? 'Ou' : 'Wala'}}</u></label>
                                            </div>
                                            <div class="form-group" ng-show="mrh.isMedication">
